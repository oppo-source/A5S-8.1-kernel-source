#
#Fingerprint
#
menu "Fingerprint support"
	depends on INPUT

config MTK_FINGERPRINT_SUPPORT
	bool "Mediatek Fingerprint"
	default n
	help
	  Fingerprint mtk driver enable/disable in the kernel.
	  Say Y here if you want to use mediatek fingerprint driver,
	  fingerprint driver will support fingerprint function

if MTK_FINGERPRINT_SUPPORT
config MTK_FINGERPRINT_SELECT
	string "sensor type"
#	default "GF318M"
        default "MULTI"

# Silead Fingerprint driver configuration

config SILEAD_FP
	tristate "Silead Fingerprint driver support"
	help
	  This supports Silead Fingerprint drivers.

config SILEAD_FP_SPI
	tristate "Silead spi Fingerprint driver support"
	select SILEAD_FP
	help
	  This supports Silead spi Fingerprint drivers.

config SILEAD_FP_PLATFORM
	tristate "Silead platformm Fingerprint driver support"
	select SILEAD_FP
	help
	  This supports Silead platform Fingerprint drivers.

config EGIS_FP
	tristate "Egis Fingerprint driver support"
	help
	  This supports Egis Fingerprint drivers.

config EGIS_FP_PLATFORM
	tristate "Egis platformm Fingerprint driver support"
	select EGIS_FP
	help
	  This supports Egis platform Fingerprint drivers.

if MTK_FINGERPRINT_SELECT = "FPC1145"
config FPC_FINGERPRINT
	bool "FPC Fingerprint"
	default y
	help
	  FPC fingerprint FPC's 102x/104x device.
endif

if MTK_FINGERPRINT_SELECT = "GF318M" ||  MTK_FINGERPRINT_SELECT = "GF5216"
config GOODIX_FINGERPRINT
	bool "Goodix Fingerprint"
	default y
	help
	  Goodix Fingerprint chip GF316M/GF318M/GF3118M/GF518M/GF5118M/GF516M/GF816M/GF3208/GF3206/
	  GF3266/GF3288/GF5206/GF5216/GF5208 TEE driver

config GOODIX_SENSOR_TYPE
	string "SENSOR_TYPE"
	default "GF5216"
	help
	  Must input sensor type, or default is GF316M GF318M GF3118M GF518M GF5118M GF516M GF816M
	  GF3208 GF3206 GF3266 GF3288 GF5206 GF5216 GF5208
endif
endif
endmenu
